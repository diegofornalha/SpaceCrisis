{
  "name": "project-u",
  "version": "0.1.0",
  "description": "An on-chain game profile management framework",
  "keywords": [
    "Web3",
    "Game",
    "Flow"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx dotenv-cli -c emulator -- nuxi dev",
    "dev:testnet": "npx dotenv-cli -c testnet -- nuxi dev",
    "build": "nuxi build",
    "preview": "nuxi preview",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "update:emulator": "npx dotenv-cli -c emulator -- flow project deploy --update",
    "update:testnet": "npx dotenv-cli -c testnet -- flow project deploy --network=testnet --update"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.4.1",
    "@onflow/transport-grpc": "^1.1.4",
    "@rollup/plugin-inject": "^5.0.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/elliptic": "^6.4.14",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vueuse/core": "^9.13.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nuxt": "^3.2.2",
    "postcss-custom-properties": "^13.1.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.5",
    "vite-plugin-node-polyfills": "^0.7.0"
  },
  "dependencies": {
    "@onflow/fcl": "1.3.2",
    "@onflow/types": "^1.0.5",
    "@upstash/redis": "^1.20.0",
    "elliptic": "^6.5.4",
    "h3-zod": "^0.3.10",
    "sha3": "^2.1.4",
    "uuid": "^9.0.0",
    "zod": "^3.20.6"
  }
}
